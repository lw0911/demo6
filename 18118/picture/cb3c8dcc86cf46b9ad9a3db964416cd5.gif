function join_buys_ajax(goods_id,obj)
{
	$.ajaxSettings.async = false; 
	$.getJSON('/goods/join_buys_add',{"goods_id":goods_id,"type_val":"1","random":Math.random},function(content){
		if(content.isError == false)
		{
			alert(content.message);
		}
		else
		{
			alert(content.message);
		}
	});
}


//商品加入购物车
function joinCart(gid,min_count)
{

//	console.log('bbb');return false;

	msg='';
	/*
	if(!checkSpecSelected())
	{
		tips('请先选择商品的规格');
		return;
	}
	*/
	
	var min_buy   = parseInt(min_count);
	var price     = parseFloat($.trim($('#real_price').text()));
	var productId = $('#product_id').val();
	var type      = productId ? 'product' : 'goods';
	//var goods_id  = (type == 'product')?productId:;
	$.ajaxSettings.async = false; 
	$.getJSON('/simple/joinCart',{"goods_id":gid,"type":type,"goods_num":min_buy,"random":Math.random},function(content){
		if(content.isError == false)
		{
			msg=content.message;
		}
		else
		{
			msg=content.message;
		}
	});
	
	return msg;
}



