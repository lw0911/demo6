			body_scroll=body_scroll+winhg;
			ajax_page_list_top=ajax_page_list_top-500;
			
			if(body_scroll >=ajax_page_list_top && is_over==0){
				
				$('#duo_ajax_page_list').attr('is_over','1');
				var jsonGoodLists=eval("("+''+")");
				var pagenum='8';
				var data_length=json_length(jsonGoodLists);
				var total_num=Math.ceil(data_length/pagenum);
				
				if(now_page>total_num){
					return false;
				}
				
				if(data_length>=0 ){
					var startnum=(now_page-1)*pagenum;
					var endnum=pagenum*now_page;
					
					for (key in jsonGoodLists)
					{
						if(key >= startnum && key <endnum){
							content=jsonGoodLists[key];
							insertGoods(content);
						}
					}
					
					$('#duo_ajax_page_list').attr('now_page',now_page+1);
